#include<stdio.h>
main()
{
	int i,j,temp;
	int arr[7]={1,3,5,6,2,3,4};
	for(int i=0;i<=6;i++){
	printf("%d",arr[i]);
}
	int n=7;
	for( int i=0;i<=n-2;i++){
			for( int j=0;j<=n-2;j++){
				if(arr[j]>arr[j+1]){
					int temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;
				}
}
}
    printf("\n");
	for(int i=0;i<=6;i++){
		printf("%d",arr[i]);
	}

}
